<h1>New entry</h1>

<% form_tag ({:action => 'create'}, :name => 'new_entry') do %>
  <%= render :partial => 'form' %>
  <%= submit_tag "Create" %>
<% end %>
<%= link_to 'Back', :action => 'list' %>
<%= javascript_tag "document.new_entry.entry_description.focus(); document.new_entry.entry_description.select();" %>
